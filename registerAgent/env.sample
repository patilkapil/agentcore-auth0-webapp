# Copy to .env in this folder and replace placeholders with actual values.

# AWS credentials consumed by boto3 sessions in deployment/runtime flows.
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
# Uncomment if you rely on temporary credentials.
# AWS_SESSION_TOKEN=your-aws-session-token
# Defaults to us-east-1 if omitted.
AWS_DEFAULT_REGION=us-east-1

# Auth0 configuration required by agentcore_deployment.py.
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_AUDIENCE=your-auth0-api-audience


